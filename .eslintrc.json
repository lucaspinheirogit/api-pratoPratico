{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "no-shadow": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "no-unused-expressions": 0,
      "no-console": 0
    }
}